  <script type="text/javascript">
  	
  	
  	function menu_collacp(myobj){
  			$(myobj).children().each(function(index,data){  				
  		    		if(index!=0){
  		    				$(data).toggle(1000);
		    			}
		    })
  	}
  	
  	
  	
		
		
	</script>
  
