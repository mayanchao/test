  <script type="text/javascript">
	//获取当前页面的ip地址 
	var applicationIp = getUrlParam("applicationIp");
	
	//获取当前应用的配置文件 
	$.ajax({
		type:"post",
		async:false,//同步获取数据
		url:"/web/project/cloud/action/MyAction_findCloudGroup.do1?applicationIp=" + applicationIp,	
		success: function(data){
			for (x in data){
				$("#global_cloud_limit_group").append(
					"<option  value='" + x + "'>"  + x + "</option>"
				);
  			}
		}
	});
			
	$.ajax({
		type:"post",
		async:false,//同步获取数据
		url:"/web/project/cloud/action/MyAction_findCloudApplicationList.do1",	
		success: function(data){
			
			$("#global_cloud_application_type").append( "<option  >please select</option>" )
			for (x in data){
				$("#global_cloud_application_type").append(
					
					"<option  value='" + data[x].typeKey + "'>"  + data[x].typeKey + "</option>"
				);
  			}
			
		}
	});
			
	$("#global_cloud_limit_button_addTasks").bind("click",function(){
		alert(1234);
		
		var myurl = "/	web/project/cloud/action/MyAction_buildCloudRun.do1?";
		  myurl +=  "applicationIp=" +  getUrlParam("applicationIp")
		  myurl +=  "&applicationType=" +  $("#global_cloud_application_type").val()
		  myurl +=  "&groupTaskId=" +  $("#global_cloud_limit_group").val()
		alert(myurl);  
		
		
			$.ajax({
		type:"post",
		async:false,//同步获取数据
		url:myurl,	
		success: function(data){
			
			
		}
	});
	});
	
	
   	/**
  	 * 通用菜单集合 
  	 * @param myobj 当前对象 
  	 * @param event 出发事件对象 
  	 */
  	function globalTools_menu_collacp(myobj,event){
		
		$(myobj).parent().children("li").each(function(index,data){			
	    	if(index!=0){
	    		$(data).toggle(1000);
    		}
	    });
		//停止冒泡
		//globalToolsStopBubble(event);
	}
	
	
	
	
	
	</script>
  
